    "redux",
    "reduxjs",
    "react-router",
    "bootstrap",
    "webpack",
    "flux",
    "jwt",
    "autho0",
    "dashboard",
    "boilerplate"
  ],
  "author": "Yunjun Mu <yunjun.mu@gmail.com> (http://github.com/cloudmu)",
